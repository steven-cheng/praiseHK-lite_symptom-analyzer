<?xml version='1.0' encoding='utf-8'?>
<widget id="hk.ust.ienv.praisehk_lite_symptom_analyzer" version="0.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <name>praisehk_lite_symptom_analyzer</name>
    <description>
        An application that analysis relationship between certain symptoms and pollutants.
    </description>
    <author email="stcheng@ust.hk" href="ienv.ust.hk">
        Steven Cheng
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="Orientation" value="portrait" />
</widget>
